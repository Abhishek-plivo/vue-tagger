{
  "name": "vue-tagger",
  "version": "1.1.0",
  "description": "Simple tagging component for Vue with built-in autocomplete.",
  "repository": "https://github.com/syropian/vue-tagger",
  "main": "src/VueTagger.vue",
  "scripts": {
    "build:docs": "vbuild ./docs/index.js",
    "lint": "eslint src --ext='.js,.vue'",
    "serve": "vbuild ./docs/index.js --dev",
    "test": "ava"
  },
  "ava": {
    "verbose": true,
    "files": [
      "tests/**/*.spec.js"
    ],
    "require": [
      "./tests/utils/setup.js"
    ]
  },
  "dependencies": {
    "awesomplete": "^1.1.1",
    "fuzzysearch": "^1.0.3",
    "lodash.differenceby": "^4.8.0"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-runtime": "^6.23.0",
    "browser-env": "^2.0.29",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-vue": "^2.0.1",
    "node-sass": "^4.5.2",
    "sass-loader": "^6.0.3",
    "vbuild": "^6.24.14",
    "vue": "^2.2.6",
    "vue-loader": "^11.3.4",
    "vue-node": "^1.1.0",
    "vue-template-compiler": "^2.2.6",
    "webpack": "^2.4.1"
  },
  "keywords": [
    "vue",
    "tags",
    "autocomplete"
  ],
  "author": "Collin Henderson",
  "license": "MIT"
}
